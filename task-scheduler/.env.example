# Task Scheduler Configuration
# For FosterbotHome orchestrator, these defaults should be used:
SCHEDULER_DEFAULT_AGENT=fosterbot
SCHEDULER_DEFAULT_RUNTIME=claude

# Storage locations
SCHEDULER_JOBS_FILE=/opt/.task-scheduler/jobs.json
SCHEDULER_LOGS_DIR=/opt/.task-scheduler/logs/
SCHEDULER_RESULTS_DIR=/opt/.task-scheduler/results/

# Daemon behavior
SCHEDULER_LOG_LEVEL=INFO
SCHEDULER_RETENTION_DAYS=30
SCHEDULER_RETRY_MAX=3
SCHEDULER_DAEMON_CHECK_INTERVAL=1
